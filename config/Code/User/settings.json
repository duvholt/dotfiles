{
  "workbench.colorTheme": "One Dark Pro",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "'Fira Code', 'Hack', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "breadcrumbs.enabled": true,
  "files.exclude": {
    "node_modules/": true
  },
  "editor.folding": false,
  "jest.pathToJest": "",
  "jest.pathToConfig": "config/jest.js",
  "todohighlight.keywords": [
    {
      "text": "NOTE:",
      "color": "#8951ba",
      "backgroundColor": "rgba(0, 0, 0, 0.3)",
      "isWholeLine": false
    },
    {
      "text": "HACK:",
      "color": "#bc382d",
      "backgroundColor": "rgba(0, 0, 0, 0.3)",
      "isWholeLine": false
    },
    {
      "text": "TODO:",
      "color": "#bcb22d",
      "backgroundColor": "rgba(0, 0, 0, 0.3)",
      "isWholeLine": false
    }
  ],
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "window.titleBarStyle": "native",
  "python.formatting.provider": "black",
  "editor.minimap.maxColumn": 50,
  "editor.minimap.renderCharacters": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "explorer",
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "typescript.surveys.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.autocompleteUnimportedPackages": true,
  "go.buildOnSave": "workspace",
  "go.inferGopath": true,
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "rust-client.channel": "nightly",
  "workbench.editor.labelFormat": "short",
  "ruby.rubocop.onSave": false,
  "liveshare.presence": true,
  "rust-client.disableRustup": true,
  "rust-analyzer.procMacro.enabled": true,
  "liveshare.allowGuestTaskControl": true,
  "workbench.startupEditor": "newUntitledFile",
  "qttools.creator": "/usr/bin/qtcreator",
  "qttools.extraSearchDirectories": ["/usr/bin/"],
  "qttools.loglevel": "debug",
  "mesonbuild.configureOnOpen": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.default.compileCommands": "build/compile_commands.json",
  "clangd.checkUpdates": true,
  "C_Cpp.workspaceSymbols": "All",
  "C_Cpp.updateChannel": "Insiders",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.testing.pytestEnabled": true,
  "files.enableTrash": false,
  "editor.scrollbar.verticalScrollbarSize": 18,
  "redhat.telemetry.enabled": false,
  "[sql]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
